# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=local_user
POSTGRES_DB=local_db
POSTGRES_PASSWORD=localpassword
DATABASE_URL=postgres://local_user:localpassword@localhost:5432/local_db

# Admin API Key for protected endpoints (required in production)
ADMIN_API_KEY=test-admin-key

# Dashboard login credentials (development)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin

# Webhook secret for cron triggers (tests)
CRON_WEBHOOK_SECRET=test-secret

# API response cache (in-memory)
API_CACHE_ENABLED=true
API_CACHE_TTL_CARGAS=20
API_CACHE_TTL_STATUS=10

# Bun API routing
API_ORIGIN=http://localhost:4000
API_PORT=4000
