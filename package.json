{
  "name": "projeto",
  "version": "1.0.0",
  "description": "<h1 align=\"center\"> DevLinks </h1>",
  "homepage": "https://github.com/jeancsla/projeto#readme",
  "bugs": {
    "url": "https://github.com/jeancsla/projeto/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeancsla/projeto.git"
  },
  "license": "MIT",
  "author": "",
  "main": "script.js",
  "scripts": {
    "dev": "npm run services:up && npm run wait-for-postgres && npm run migration:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:eslint:check": "eslint .",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "npm run services:up && concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand --verbose\"",
    "test:watch": "jest --watchAll --runInBand",
    "migration:create": "node-pg-migrate --migrations-dir infra/migrations create",
    "migration:up": "node-pg-migrate --migrations-dir infra/migrations --envPath .env.development up",
    "migration:down": "node-pg-migrate --migrations-dir infra/migrations down",
    "wait-for-postgres": "node infra/scripts/wait-for-postgres.js",
    "prepare": "husky",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,env,yml,yaml}": [
      "secretlint"
    ]
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "cheerio": "^1.2.0",
    "claude": "^0.1.1",
    "dotenv": "^16.4.4",
    "dotenv-expand": "^11.0.6",
    "eslint-config-next": "^16.1.6",
    "next": "^16.1.6",
    "node-cron": "^3.0.3",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^29.14.0",
    "jest": "^29.6.2",
    "prettier": "^3.7.4",
    "@commitlint/cli": "^19.3.0",
    "husky": "^9.1.4",
    "@commitlint/config-conventional": "^19.2.2",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.1",
    "secretlint": "^11.3.1"
  }
}
